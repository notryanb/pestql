WHITESPACE = _{ " " }
keyword = { ^"select" | ^"from" | ^"where" }
selection = @{ (ASCII_ALPHANUMERIC | "_")+ ~ ("," ~ selection)* | "*" }
terminator = { ";" }
sql = { (keyword ~ selection)+ ~ terminator }


// Experimental
column = @{ ("*" ~ !".") | (ASCII_ALPHANUMERIC | "_")+ ~ ("." ~ (ASCII_ALPHANUMERIC | "_" | "*")+)* }
column_selection = { column+ ~ ("," ~ column)+ ~ !"," }